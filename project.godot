config_version=5

[ResPicker]

do_register_resources=true

[SafeStrings]

scenes/scene_associations={
"res://game/ball/ball.tscn": "res://game/ball/Ball.cs",
"res://game/load_abilities/dive_spender/dive_spender.tscn": "res://game/load_abilities/dive_spender/DiveSpender.cs",
"res://game/player/player.tscn": "res://game/player/Player.cs",
"res://singletons/transitions/transitions.tscn": "res://singletons/transitions/Transitions.cs",
"res://ui/main_menu/main_menu.tscn": "res://ui/main_menu/MainMenu.cs"
}

[application]

config/name="Kaizemo 4"
run/main_scene="res://ui/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.0", "C#", "Mobile")

[autoload]

InputManager="*res://singletons/InputManager.cs"
Transitions="*res://singletons/transitions/transitions.tscn"

[display]

window/size/viewport_width=416
window/size/viewport_height=234
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="canvas_items"
window/quality/screen_space_aa=1

[dotnet]

project/assembly_name="Kaizemo 4"

[editor]

scene/scene_naming=1

[editor_plugins]

enabled=PackedStringArray("res://addons/CSResourePicker/plugin.cfg", "res://addons/SafeStrings/plugin.cfg", "res://addons/BetterFileSystem/plugin.cfg")

[gui]

theme/custom="res://ui/common/main_theme.theme"
